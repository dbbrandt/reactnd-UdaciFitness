{
  "expo": {
    "name": "Udacity Fitness App",
    "description": "The best triathlon training app in the world.",
    "slug": "Udacifitness",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/triathlon-512.png",
    "notification": {
      "icon": "./assets/triathlon-45.png"
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "ios": {
      "bundleIdentifier": "org.udacifitness.exp",
    },
    "android": {
      "package": "org.udacifitness.exp"
    }
  }
}
